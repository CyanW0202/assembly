#include <P18F4321.INC>

MOVLW 0x0F;
MOVF ADCON1;
BSF INTCON, INT0IE;IE, INT2IP = 0, 
BCF INTCON, INT0IF; INT2IF
BSF INTCON3, INT1IE;
BSF INTCON3, INT1IF;
BCF INTCON3, INT1IP;
BSF INTCON3, INT2IE;
BSF INTCON3, INT2IF;
BCF INTCON3, INT2IP;

MOVLW 0x40;0100 0000
MOVF INCON2;
    
;INTEDGE 
;MOVLW 0xA0;
;MOVF T1CON;
;MOVLW 0xFF;
;MOVF TMR1H;
;MOVFW 0x06;
;MOVF TRM1L;
;BCF  PIR1, TMR1IF;
BSF RCON, IPEN;
BSF INTCON, GIEH; &GIEL, IPEN
BSF INTCON, GIEL;
    
 


